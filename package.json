{
  "name": "ew-react-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@craco/craco": "^7.0.0",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.11.0",
    "@mui/lab": "^5.0.0-alpha.114",
    "@mui/material": "^5.11.3",
    "@mui/x-date-pickers-pro": "^5.0.12",
    "@tanstack/react-query": "^4.20.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.11",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "axios": "^1.2.2",
    "date-fns": "^2.29.3",
    "firebase": "^9.15.0",
    "lodash": "^4.17.21",
    "query-string": "^8.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-firebase-hooks": "^5.1.1",
    "react-hook-form": "^7.41.3",
    "react-router": "^6.6.1",
    "react-router-dom": "^6.6.1",
    "react-scripts": "5.0.1",
    "react-spinners": "^0.13.7",
    "react-use": "^17.4.0",
    "sanitize-html": "^2.8.1",
    "typescript": "^4.9.4",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "craco eject",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx ./src",
    "ts": "tsc -p ./tsconfig.json"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {
      "import/order": [
        "warn",
        {
          "newlines-between": "ignore",
          "pathGroups": [
            {
              "pattern": "react",
              "group": "external",
              "position": "before"
            },
            {
              "pattern": "react**",
              "group": "external",
              "position": "before"
            },
            {
              "pattern": "@tanstack/**",
              "group": "external",
              "position": "before"
            },
            {
              "pattern": "@material-ui/**",
              "group": "external",
              "position": "before"
            },
            {
              "pattern": "core/types/**",
              "group": "internal",
              "position": "before"
            },
            {
              "pattern": "core/helpers/**",
              "group": "internal",
              "position": "before"
            },
            {
              "pattern": "core/clients/**",
              "group": "internal",
              "position": "before"
            },
            {
              "pattern": "core/contexts/**",
              "group": "internal",
              "position": "before"
            },
            {
              "pattern": "core/hooks/**",
              "group": "internal",
              "position": "before"
            },
            {
              "pattern": "core/components/**",
              "group": "internal",
              "position": "before"
            },
            {
              "pattern": "core/**",
              "group": "internal",
              "position": "before"
            },
            {
              "pattern": "shared/types/**",
              "group": "internal",
              "position": "before"
            },
            {
              "pattern": "shared/helpers/**",
              "group": "internal",
              "position": "before"
            },
            {
              "pattern": "shared/contexts/**",
              "group": "internal",
              "position": "before"
            },
            {
              "pattern": "shared/hooks/**",
              "group": "internal",
              "position": "before"
            },
            {
              "pattern": "shared/components/**",
              "group": "internal",
              "position": "before"
            },
            {
              "pattern": "shared/img/**",
              "group": "internal",
              "position": "before"
            },
            {
              "pattern": "shared/**",
              "group": "internal",
              "position": "before"
            },
            {
              "pattern": "modules/*/types/**",
              "group": "internal",
              "position": "before"
            },
            {
              "pattern": "modules/*/helpers/**",
              "group": "internal",
              "position": "before"
            },
            {
              "pattern": "modules/*/contexts/**",
              "group": "internal",
              "position": "before"
            },
            {
              "pattern": "modules/*/hooks/**",
              "group": "internal",
              "position": "before"
            },
            {
              "pattern": "modules/*/components/**",
              "group": "internal",
              "position": "before"
            },
            {
              "pattern": "modules/*/views/**",
              "group": "internal",
              "position": "before"
            },
            {
              "pattern": "modules/**",
              "group": "internal",
              "position": "before"
            },
            {
              "pattern": "config",
              "group": "internal",
              "position": "before"
            },
            {
              "pattern": "tracking",
              "group": "internal",
              "position": "before"
            },
            {
              "pattern": "pages/**",
              "group": "internal",
              "position": "before"
            },
            {
              "pattern": "styles/**",
              "group": "internal",
              "position": "before"
            },
            {
              "pattern": "../**",
              "group": "internal",
              "position": "after"
            },
            {
              "pattern": "./**",
              "group": "internal",
              "position": "after"
            }
          ],
          "pathGroupsExcludedImportTypes": [
            "builtin"
          ]
        }
      ],
      "@typescript-eslint/camelcase": "off",
      "@typescript-eslint/no-empty-function": "off",
      "@typescript-eslint/explicit-function-return-type": "off",
      "@typescript-eslint/ban-ts-ignore": "off",
      "@typescript-eslint/no-explicit-any": "off",
      "jsx-quotes": [
        "warn",
        "prefer-double"
      ],
      "comma-dangle": [
        "warn",
        "always-multiline"
      ],
      "indent": [
        "warn",
        2
      ],
      "object-curly-spacing": [
        "warn",
        "always"
      ],
      "quotes": [
        "warn",
        "single"
      ],
      "semi": [
        "error"
      ],
      "react/jsx-curly-brace-presence": [
        1,
        "never"
      ],
      "react/prop-types": "off",
      "@typescript-eslint/no-var-requires": "off",
      "no-restricted-imports": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/lodash": "^4.14.191",
    "@types/sanitize-html": "^2.8.0"
  }
}
